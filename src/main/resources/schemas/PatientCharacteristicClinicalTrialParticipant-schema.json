{
  "$schema" : "https://json-schema.org/draft/2019-09/schema",
  "$defs" : {
    "Boolean" : {
      "type" : "object",
      "properties" : {
        "value" : {
          "type" : "boolean"
        }
      }
    },
    "Code" : {
      "type" : "object",
      "properties" : {
        "code" : {
          "$ref" : "#/$defs/String"
        },
        "display" : {
          "$ref" : "#/$defs/String"
        },
        "system" : {
          "$ref" : "#/$defs/String"
        },
        "version" : {
          "$ref" : "#/$defs/String"
        }
      }
    },
    "DateTime" : {
      "type" : "object",
      "properties" : {
        "value" : {
          "type" : "string"
        }
      }
    },
    "String" : {
      "type" : "object",
      "properties" : {
        "value" : {
          "type" : "string"
        }
      }
    }
  },
  "type" : "object",
  "properties" : {
    "code" : {
      "$ref" : "#/$defs/Code"
    },
    "id" : {
      "$ref" : "#/$defs/String"
    },
    "patientId" : {
      "$ref" : "#/$defs/String"
    },
    "reason" : {
      "$ref" : "#/$defs/Code"
    },
    "relevantPeriod" : {
      "type" : "object",
      "properties" : {
        "high" : {
          "$ref" : "#/$defs/DateTime"
        },
        "highClosed" : {
          "$ref" : "#/$defs/Boolean"
        },
        "low" : {
          "$ref" : "#/$defs/DateTime"
        },
        "lowClosed" : {
          "$ref" : "#/$defs/Boolean"
        }
      }
    }
  }
}